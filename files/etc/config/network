
config interface 'loopback'
        option device 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'

config globals 'globals'

config interface 'lan'
        option device 'lan3'
        option proto 'static'
        option ipaddr '192.168.4.1'
        option netmask '255.255.255.0'

config interface 'openvpn'
        option proto 'none'
        option device 'tun0'

config interface 'wwan0'
        option proto 'modemmanager'
        option device '/sys/devices/platform/soc/11200000.usb/usb1/1-1/1-1.3'
        option apn 'internet'
        option auth 'none'
        option iptype 'ipv4v6'
        option loglevel 'ERR'
        option metric '1'

config interface 'wwan1'
        option proto 'modemmanager'
        option device '/sys/devices/platform/soc/11200000.usb/usb1/1-1/1-1.2'
        option apn 'internet'
        option auth 'none'
        option iptype 'ipv4v6'
        option loglevel 'ERR'
        option metric '2'

config interface 'wan1'
        option proto 'dhcp'
        option device 'wan'
        option metric '3'
        option peerdns '0'

config interface 'wan2'
        option proto 'dhcp'
        option device 'lan1'
        option metric '4'
        option peerdns '0'

config interface 'wan3'
        option proto 'dhcp'
        option device 'lan2'
        option metric '5'
        option peerdns '0'

config interface 'wan4'
        option proto 'dhcp'
        option device 'eth1'
        option metric '6'
        option peerdns '0'

config interface 'wan5'
        option proto 'dhcp'
        option device 'eth2'
        option metric '7'
        option peerdns '0'
